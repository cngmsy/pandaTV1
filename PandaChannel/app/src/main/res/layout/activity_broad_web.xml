<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

<WebView
    android:id="@+id/broad_webview"
    android:layout_width="match_parent"
    android:layout_height="1200px"></WebView>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/broad_collect"
            android:layout_width="52px"
            android:layout_height="52px"
            android:layout_marginLeft="300px"
            android:background="@drawable/personal_shoucang_img"
            />
        <ImageView
            android:id="@+id/broad_share"
            android:layout_marginLeft="60px"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/pe_share_img"
            />
    </LinearLayout>
</LinearLayout>